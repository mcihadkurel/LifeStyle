<h1>New Article</h1>
<%= form_with(model: @article, local: true) do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.label :description %>
    <%= f.text_area :text %>

    <%= f.label :image_file %>
    <%= f.text_field :image_file %>

    <%= f.submit %>
<% end %>


<%= link_to 'Back', articles_path %>

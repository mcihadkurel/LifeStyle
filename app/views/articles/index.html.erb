<%= render 'header' %>

<ul>
<% @articles.each do |article| %>
<li>
<strong><%= article.title %></strong>
<%= article.text %>
<%= article.author_id %>
</li>
<% end %>
</ul>
<%= link_to "Edit", edit_article_path(:id), class: "button" %>
<%= link_to "Delete", @article, class: "button", method: :delete %>
<br>
<%= link_to "Add New Article", new_article_path, class: "button" %>
<%= render 'footer' %>



